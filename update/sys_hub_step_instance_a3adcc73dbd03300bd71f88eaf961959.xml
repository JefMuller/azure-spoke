        <sys_updated_on>2019-03-26 17:40:02</sys_updated_on>
        <value>(function execute(inputs, outputs) {
  	outputs.path = "/"+inputs.subscriptionId+"/resourceGroups/"+inputs.resourceGroupName+"/providers/Microsoft.Network/publicIPAddresses/"+inputs.name+"?api-version=" + inputs.apiVersion;
	outputs.body = `
      {
        "properties": {
          "publicIPAllocationMethod": "`+inputs.publicIPAllocationMethod+`",
          "idleTimeoutInMinutes": `+inputs.idleTimeoutInMinutes+`,
          "publicIPAddressVersion": "`inputs.publicIPAddressVersion`"
        },
        "sku": {
          "name": "`+inputs.skuName+`"
        }
      }`;
})(inputs, outputs);</value>
